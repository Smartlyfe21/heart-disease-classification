   "source": [
    "np.random.seed(42)\n",
    "\n",
    "\n",
    "#single trining of 5 fold cross validation\n",
    "clf_single_score = clf.score(x_test, y_test)\n",
    "\n",
    "#mean of 5 fold\n",
    "clf_cross_val_score = np.mean(cross_val_score(clf, x, y, cv=5))\n",
    "\n",
    "#compare the two\n",
    "clf_single_score, clf_cross_val_score"
   ]
  },
